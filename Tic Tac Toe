def print_board(board):
    
    def print_line(line):
        to_print("|")
        for x in line:
            to_print += (x+"|")
        print(to_print)
    
    h_line = "="*9
    print(h_line)
    for x in board:
        print_line(x)
    print(h_line)
    
board = [["X","O",""],["O","","X"],["","","X"]]

print_board(board)