def print_board(board):
    
    def print_line(line):
        print("|")
        for x in line:
            print(line(x)+"|")
    
    h_line = "="*9
    print(h_line)
    for x in board:
        print_line(x)
    print(h_line)
    
board = [["X","O",""],["O","","X"],["","","X"]]

print_board(board)